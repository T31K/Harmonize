{
  "$schema": "../node_modules/@tauri-apps/cli/schema.json",
  "build": {
    "beforeBuildCommand": "pnpm build",
    "beforeDevCommand": "pnpm dev",
    "devPath": "http://localhost:3000",
    "withGlobalTauri": true,
    "distDir": "../dist"
  },
  "package": {
    "productName": "tauri-react-template",
    "version": "0.1.0"
  },
  "tauri": {
    "allowlist": {
      "globalShortcut": {
        "all": true
      },
      "window": {
        "all": true
      },
      "shell": {
        "all": true,
        "execute": false,
        "open": false,
        "scope": [],
        "sidecar": false
      }
    },
    "bundle": {
      "active": true,
      "category": "DeveloperTool",
      "copyright": "",
      "deb": {
        "depends": []
      },
      "externalBin": [],
      "icon": [
        "icons/32x32.png",
        "icons/128x128.png",
        "icons/128x128@2x.png",
        "icons/icon.icns",
        "icons/icon.ico"
      ],
      "identifier": "com.tarui-react-template.dev",
      "longDescription": "",
      "macOS": {
        "entitlements": null,
        "exceptionDomain": "",
        "frameworks": [],
        "providerShortName": null,
        "signingIdentity": null
      },
      "resources": [],
      "shortDescription": "",
      "targets": "all",
      "windows": {
        "certificateThumbprint": null,
        "digestAlgorithm": "sha256",
        "timestampUrl": ""
      }
    },
    "security": {
      "csp": null
    },
    "updater": {
      "active": false
    },
    "macOSPrivateApi": true,
    "windows": [
      {
        "fullscreen": false,
        "width": 550,
        "height": 550,
        "focus": true,
        "transparent": true,
        "hiddenTitle": true,
        "decorations": false,
        "titleBarStyle": "Transparent",
        "resizable": false,
        "center": true
      }
    ]
  }
}
